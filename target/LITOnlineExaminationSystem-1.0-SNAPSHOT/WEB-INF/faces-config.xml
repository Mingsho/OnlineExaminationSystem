<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    
    <navigation-rule>
        <from-view-id>/pages/user/CreateUser.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>success</from-outcome>
            <to-view-id>/pages/user/CreateUserAccount.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>failure</from-outcome>
            <to-view-id>/pages/ErrorPage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/Authentication.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>administrators</from-outcome>
            <to-view-id>/pages/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>examinee</from-outcome>
            <to-view-id>/pages/StudentWelcomePage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/StudentWelcomePage.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>start</from-outcome>
            <to-view-id>/pages/ExaminationPage.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>failed</from-outcome>
            <to-view-id>/pages/ErrorPage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/course/EnrolStudents.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>error</from-outcome>
            <to-view-id>/pages/ErrorPage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
